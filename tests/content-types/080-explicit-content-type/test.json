{
  "id": "080-explicit-content-type",
  "name": "Explicit Content-Type",
  "description": "File part with explicitly specified Content-Type header",
  "spec_references": [
    "RFC 7578 Section 4.4"
  ],
  "category": "content-types",
  "tags": [
    "required",
    "parsing"
  ],
  "expected": {
